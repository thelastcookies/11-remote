#plant_cursor{
	-moz-transform:scaleX(0.67);       
    -webkit-transform:scaleX(0.67);   
    transform:scaleX(0.67);
}
.cursor_1{
    background-image: url(../img/Group/Group_plant_cursor.png);
    animation: mymove 2s linear infinite;
    -webkit-animation: mymove 2s linear infinite; /* Safari and Chrome */
}
.cursor_2{
    background-image: url(../img/Group/Group_plant_cursor.png);
    animation: mymove_2 2s linear infinite;
    -webkit-animation: mymove_2 2s linear infinite; /* Safari and Chrome */
}
@keyframes mymove {
    0% {left: 55px;top:55px;width:1px;height:1px; opacity: 1;}
    50% {left: 28px;top:28px;width:55px;height:55px; opacity: 1;}
    100% {left: 0px;top:0px;width:109px;height:109px; opacity: 0;}
}

@-webkit-keyframes mymove /* Safari and Chrome */
{
    0% {left: 55px;top:55px;width:1px;height:1px; opacity: 1;}
    50% {left: 28px;top:28px;width:55px;height:55px; opacity: 1;}
    100% {left: 0px;top:0px;width:109px;height:109px; opacity: 0;}
}
@keyframes mymove_2 {
    0% {left: 28px;top:28px;width:55px;height:55px; opacity: 1;}
    50% {left: 0px;top:0px;width:109px;height:109px; opacity: 0;}
    51% {left: 55px;top:55px;width:1px;height:1px; opacity: 1;}
    100% {left: 28px;top:28px;width:55px;height:55px; opacity: 1;}
}

@-webkit-keyframes mymove_2 /* Safari and Chrome */
{
    0% {left: 28px;top:28px;width:55px;height:55px; opacity: 1;}
    50% {left: 0px;top:0px;width:109px;height:109px; opacity: 0;}
    51% {left: 55px;top:55px;width:1px;height:1px; opacity: 1;}
    100% {left: 28px;top:28px;width:55px;height:55px; opacity: 1;}
}